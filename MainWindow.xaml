<Window x:Class="SimpleWPFApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Simple WPF App" Height="200" Width="300">
    <Grid>
        <Frame x:Name="mainFrame"></Frame>
        <Button x:Name="myButton" Content="Click Me" HorizontalAlignment="Center" VerticalAlignment="Center" Click="myButton_Click"/>
        <Label x:Name="myLabel" Content="Hello, WPF!" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,50,0,0"/>
    </Grid>
</Window>
